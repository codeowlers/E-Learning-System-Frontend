{
  "homepage": "http://learnplus.live/",
  "name": "fyp-fe",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "prod": "next start",
    "dev:up": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "prod:up": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "restart": "docker-compose down && docker image rm fyp-fe_fyp-fe && yarn run prod:up",
    "prod:test": "docker-compose down && docker image rm fyp-fe_fyp-fe && git pull && yarn run prod:up"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.5",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.5.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "aos": "^3.0.0-beta.6",
    "axios": "^0.26.0",
    "firebase": "8.2.3",
    "formik": "^2.2.9",
    "moment": "^2.29.1",
    "next": "12.0.8",
    "pure-react-carousel": "^1.28.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-phone-input-2": "^2.15.0",
    "react-player": "^2.10.0",
    "react-redux": "^7.2.6",
    "react-uuid": "^1.0.2",
    "redux-persist": "^6.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.0.8",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.23"
  }
}
